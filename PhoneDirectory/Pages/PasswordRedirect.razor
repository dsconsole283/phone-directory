@page "/PasswordRedirect"
@inject NavigationManager navManager

<html>
  <head>
    <PageTitle>Password Reset Not Allowed</PageTitle>
    <link rel="stylesheet" href="/css/site.css"/>
  </head>
  <body>
    <p>Please contact support to have your password reset</p>
    <br />
    <br />
    <button class="return-button" @onclick="ReturnHome">Return to Directory</button>
  </body>
</html>


@code {
  private void ReturnHome()
  {
    navManager.NavigateTo("/");
  }
}
